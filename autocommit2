# /usr/bin/git add -A /home/pedro/.local/scripts/
# /usr/bin/git commit -m "Commit at every minute"
# /usr/bin/git push origin main
# git branch -M main

git init ~/.local/scripts/
git add ~/.local/scripts/
git commit -m "first commit"
git branch -M main
git remote add origin git@github.com:PedroVSCoimbra/scripts.git
git push -u origin main
